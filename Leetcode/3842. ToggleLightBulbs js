let bulbs = [10, 30, 20, 10];

var toggleLightBulbs = function (bulbs) {
    let freq = new Map();
    let result = [];

    for (let i = 0; i < bulbs.length; i++) {
        freq.set(bulbs[i], (freq.get(bulbs[i]) || 0) + 1);
    }

    for (let [bulb, count] of freq) {
        if (count % 2 !== 0) {
            result.push(bulb);
        }
    }

    return result.sort((a, b) => a - b);
};

console.log(toggleLightBulbs(bulbs));
